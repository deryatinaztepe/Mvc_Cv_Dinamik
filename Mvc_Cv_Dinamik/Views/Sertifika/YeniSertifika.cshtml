@model Mvc_Cv_Dinamik.Models.Entitiy.Tbl_Sertifikalar
@{
    ViewBag.Title = "YeniSertifika";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}

<br />
<h4> Yeni Sertifika Eekleme Sayfası</h4>
<br />
<form method="post">

    @Html.Label("Sertfika Açıklaması ")
    @Html.TextBoxFor(x => x.Aciklama, new { @class = "form-control" })
    <br />
    @Html.Label("Tarih")
    @Html.TextBoxFor(x => x.Tarih, new { @class = "form-control" })
    <br />


    <button class="btn btn-primary">Kaydet</button>
</form>






