@model Mvc_Cv_Dinamik.Models.Entitiy.Tbl_Yetenekler 
@{
    ViewBag.Title = "YeniYetenek";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}

<br />
<h4>Yeni Yetenek Ekleme Sayfası</h4>
<br />
<form method="post">
   
    @Html.Label("Yetenek ")
    @Html.TextBoxFor(x => x.Yetenek, new { @class = "form-control" })
    <br />
    @Html.Label("Derece")
    @Html.TextBoxFor(x => x.Oran, new { @class = "form-control" })
    <br />
   

    <button class="btn btn-info">Ekle</button>
</form>
